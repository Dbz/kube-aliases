#!/bin/bash

function usage {
  printf "Usage: $0: khelp [af]\n"
  printf "\ta\tdisplay generated aliases\n"
  printf "\tf\tdisplay functions/scripts\n"
  printf "\th\tdisplay khelp help\n"
}

while getopts "afh" o; do
  case "${o}" in
    a)
      cat ${KALIAS}/aliases
      exit 0
      ;;
    f)
      ls ${KALIAS}/bin
      exit 0
      ;;
    h)
      usage;
      exit 0
      ;;
    \?)
      usage
      exit 1
      ;;
  esac
done
